@model List<Car>;

@foreach (Car car in Model)

{
    <h1>@car.CarName</h1>

    @Html.ActionLink("Update Car", "UpdateCar", "Home", new { car.CarId });
    @Html.ActionLink("Delete Car", "DeleteCar", "Home", new { car.CarId }, new { onclick = $"return confirm('Are you sure you wish to delete {car.CarName}?');" });
}